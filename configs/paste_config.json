{
  "paste": {
    "root": ".",
    "out_dir": "paste_out",
    "max_lines": 4000,
    "allow_binary": false,
    "filename_search": true,
    "include": ["*.*", "**/*.*"],
    "exclude": [
      ".env",
      ".editorconfig",
      ".gitattributes",
      ".gitignore",
      ".pastechunksrc",
      ".prettierrc",
      ".vscode/settings.json",
      "README_DEV.md"
    ],
    "only_globs": [],
    "skip_globs": []
  }
}
